{
  "name": "photoalbum2022",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": "16.15.1"
  },
  "scripts": {
    "dev": "nodemon",
    "build": "next build && tsc --project tsconfig.server.json",
    "start": "set NODE_ENV=production && node ./dist/index.js",
    "lint": "next lint",
    "format": "prettier --write .",
    "heroku-postbuild": "npm run build"
  },
  "nodemonConfig": {
    "watch": [
      "server"
    ],
    "exec": "ts-node --project tsconfig.server.json server/index.ts",
    "ext": "js ts"
  },
  "dependencies": {
    "@emotion/react": "11.9.3",
    "@emotion/styled": "11.9.3",
    "@mui/icons-material": "^5.10.14",
    "@mui/material": "5.8.7",
    "@react-google-maps/api": "^2.12.1",
    "@react-spring/web": "^9.6.1",
    "axios": "^0.27.2",
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "flickr-sdk": "^6.0.0",
    "next": "12.2.1",
    "next-i18next": "^11.0.0",
    "nodemon": "^2.0.20",
    "prohairesis": "1.2.5",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "sass": "^1.53.0",
    "ts-node": "^10.9.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/mysql": "^2.15.21",
    "@types/node": "18.0.3",
    "@types/react": "18.0.15",
    "@types/react-dom": "18.0.6",
    "eslint": "8.19.0",
    "eslint-config-next": "12.2.1",
    "eslint-config-prettier": "8.5.0",
    "prettier": "2.6.2",
    "typescript": "4.7.4"
  },
  "resolutions": {
    "terser": "3.14.1"
  }
}
